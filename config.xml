<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="20124" id="com.kiatnakinbank.kkebanking" version="4.11.0" xmlns="http://www.w3.org/ns/widgets">
    <description>
        KKP e-Banking application.
    </description>
    <author email="developer@kiatnakin.co.th" href="https://ebanking.kiatnakin.co.th/ebanking">
        KKP RIB Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="mailto:*" />
    <access launch-external="yes" origin="tel:*" />
    <access launch-external="yes" origin="sms:*" />
    <access origin="*.google.com" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <access origin="*.kiatnakin.co.th" />
    <access origin="*.kkpfg.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="srvendpoint" value="https://ebankingapi.kkpfg.com/mobileapi" />
    <preference name="srvendpointversion" value="v1" />
    <preference name="openssl" value="true" />
    <preference name="urlpinning" value="https://ebankingapi.kkpfg.com/pinning" />
    <platform name="android">
        <name short="KKPeBanking">KKPeBanking</name>
        <allow-intent href="market:*" />
        <icon src="res/icon/android/icon-96-xhdpi.png" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <splash src="res/screen/android/splash.9.png" />
        <preference name="FadeSplashScreen" value="false" />
        <preference name="ShowSplashScreenSpinner" value="false" />
        <preference name="android-minSdkVersion" value="23" />
        <preference name="android-targetSdkVersion" value="28" />
    </platform>
    <platform name="ios">
        <name short="KKPeBanking">KKeBanking</name>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="res/icon/ios/icon.png" width="57" />
        <icon height="114" src="res/icon/ios/icon@2x.png" width="114" />
        <icon height="40" src="res/icon/ios/icon-40.png" width="40" />
        <icon height="80" src="res/icon/ios/icon-40@2x.png" width="80" />
        <icon height="50" src="res/icon/ios/icon-50.png" width="50" />
        <icon height="100" src="res/icon/ios/icon-50@2x.png" width="100" />
        <icon height="120" src="res/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" src="res/icon/ios/icon-60@3x.png" width="180" />
        <icon height="72" src="res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72@2x.png" width="144" />
        <icon height="76" src="res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="167" src="res/icon/ios/icon-83.5@2x.png" width="167" />
        <icon height="1024" src="res/icon/ios/icon-1024.png" width="1024" />
        <icon height="29" src="res/icon/ios/icon-small.png" width="29" />
        <icon height="58" src="res/icon/ios/icon-small@2x.png" width="58" />
        <icon height="87" src="res/icon/ios/icon-small@3x.png" width="87" />
        <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
        <preference name="FadeSplashScreen" value="false" />
        <preference name="ShowSplashScreenSpinner" value="false" />
        <preference name="BackupWebStorage" value="local" />
        <resource-file src="GoogleService-Info.plist" />
    </platform>
    <engine name="android" spec="^6.0.0" />
    <engine name="ios" spec="^4.5.5" />
    <plugin name="com.darktalker.cordova.screenshot" spec="~0.1.5" />
    <plugin name="cordova-plugin-splashscreen" spec="~3" />
    <plugin name="cordova-plugin-whitelist" spec="~1" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^6.0.8">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="Allow access to your camera so we can help make a transaction even faster." />
        <variable name="$CAMERA_USAGE_DESCRIPTION" value="Allow access to your camera so we can help make a transaction even faster." />
    </plugin>
    <plugin name="org.devgeeks.Canvas2ImagePlugin" spec="git+https://github.com/devgeeks/Canvas2ImagePlugin.git" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.2" />
    <plugin name="cordova-plugin-appversion" spec="~1.0.0" />
    <plugin name="cordova-plugin-jailbreak-detection" spec="https://github.com/leecrossley/cordova-plugin-jailbreak-detection.git" />
    <plugin name="cordova-plugin-root-detection" spec="~0.1.1" />
    <plugin name="cordova-plugin-datepicker" spec="~0.9.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.0" />
    <plugin name="cordova-plugin-uniquedeviceid" spec="https://github.com/Paldom/UniqueDeviceID.git" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.3" />
    <plugin name="cordova-plugin-sslcertificatechecker" spec="~5.1.0" />
    <plugin name="cordova-plugin-badge" spec="^0.8.5" />
    <plugin name="phonegap-plugin-push" spec="^2.1.2">
        <variable name="SENDER_ID" value="85075801930" />
        <variable name="FCM_VERSION" value="11.0.1" />
    </plugin>
    <plugin name="cordova-plugin-iroot" spec="~0.8.0">
        <variable name="ENABLE_BUSYBOX_CHECK" value="true" />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="~2.0.1" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="cordova-plugin-contacts" spec="^2.3.1" />
    <plugin name="cordova-plugin-android-permissions" spec="~1.0.0" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.4.1" />
    <plugin name="cordova-plugin-advanced-http" spec="^2.0.9" />
    <plugin name="cordova-plugin-globalization" spec="^1.11.0" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-customconfigparameters" spec="^3.0.0" />
    <plugin name="cordova-plugin-crypt-file" spec="^1.3.3" />
    <plugin name="cordova-plugin-RibFaceRecog" spec="file:../../Plugin/RibFaceRecog" />
    <plugin name="cordova-plugin-biometric-authen" spec="file:../../Plugin/BiometricAuthen">
        <variable name="FACEID_USAGE_DESCRIPTION" value="KKP e-Banking will use Face ID to confirm your identity when approving login request instead of MyPIN." />
    </plugin>
</widget>

